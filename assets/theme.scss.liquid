-ms-transition: all .4s ease;
-o-transition: all .4s ease;
transition: all .4s ease;
}

@include media-query($large-up) {
    .hover-fade-in:hover .dyno-bg-overlay {
        opacity: .7 !important;
    }
}

.hover-fade-out .dyno-bg-overlay {
  -webkit-transition: all .4s ease;
-moz-transition: all .4s ease;
-ms-transition: all .4s ease;
-o-transition: all .4s ease;
transition: all .4s ease;
}

@include media-query($large-up) {
    .hover-fade-out:hover .dyno-bg-overlay {
        opacity: 0 !important;
    }
}

.hover-zoom-in .dyno_img-bg {
-webkit-transition: all 1s ease;
-moz-transition: all 1s ease;
-ms-transition: all 1s ease;
-o-transition: all 1s ease;
transition: all 1s ease;
}

@include media-query($large-up) {
    .hover-zoom-in:hover .dyno_img-bg{
        -moz-transform: scale(1.1);
        -webkit-transform: scale(1.1);
        -o-transform: scale(1.1);
        -ms-transform: scale(1.1);
        transform: scale(1.1);
    }
}

.hover-zoom-out .dyno_img-bg{
  -moz-transform: scale(1.1);
-webkit-transform: scale(1.1);
-o-transform: scale(1.1);
-ms-transform: scale(1.1);
transform: scale(1.1);
-webkit-transition: all 1s ease;
-moz-transition: all 1s ease;
-ms-transition: all 1s ease;
-o-transition: all 1s ease;
transition: all 1s ease;
}

@include media-query($large-up) {
    .hover-zoom-out:hover .dyno_img-bg{
        -moz-transform: scale(1);
