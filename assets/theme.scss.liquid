/*================ Degradation(overwritten) Variables ================*/
// Topbar or Header
$topbar-responsive-bg: #fff;
$topbar-links: red;
$topbar-responsive-animation-type: fade-in; // or use slide-down or none
// Site Footer
$colorFooterBg: #000;
$colorFooterText: #fff;
$colorFooterMenuHeader: #ccc;
$colorFooterMenuLink: #fff;
// Site Utility
$colorUtilityBg: #fff;
$colorUtilityText: #333;
// Borders
$colorBordersForms: #888;
$colorBorders: #ccc;
// Button Colors
$colorBtnPrimary: red;
$colorBtnPrimaryHover: darken($colorBtnPrimary, 10%);
$colorBtnPrimaryActive: darken($colorBtnPrimaryHover, 10%);
$colorBtnPrimaryText: #fff;
$colorBtnSecondary: red;
$colorBtnSecondaryHover: darken($colorBtnSecondary, 10%);
$colorBtnSecondaryActive: darken($colorBtnSecondaryHover, 10%);
$colorBtnSecondaryText: #fff;
// Basic Headers & Text
$colorHeader: #000;
$colorText: #333;
$colorHyperlinkText: blue;
$page-banner: rgba(0,0,0,0.1);

/*================ Color Variables ================*/
$color-primary: {{ settings.color_primary }};
$color-secondary: {{ settings.color_secondary }};
$color-low-gray: {{ settings.color_low_gray }};
$color-mid-gray: {{ settings.color_mid_gray }};
$color-high-gray: {{ settings.color_high_gray }};
$color-low-mid-gray: lighten($color-mid-gray, 30%);
$color-high-mid-gray: darken($color-mid-gray, 15%);
$color-white: {{ settings.color_low_gray }};

/*================ Typography Variables ================*/
$fontHeadline: {{ settings.font_headline }};
$fontAccent: {{ settings.font_accent }};
$fontParagraph: {{ settings.font_paragraph }};
$fontBase: {{ settings.type_base_size }};
$font-weight-normal: 400;
$font-weight-bold: 700;

/*================ Sizing/Layout Variables ================*/
$sitewidthmax: {{ settings.display_site_width; }}px;
$sitecontentwidthmax: {{ settings.display_content_width; }}px;
$sitetextwidthmax: {{ settings.display_text_width; }}px;
$gutter: 30px;


/*============================================================================
  Grid Breakpoints and Class Names
    - Do not change the variable names
==============================================================================*/
$grid-medium: {{settings.display_breakpoint_medium}}px;
$grid-large: {{settings.display_breakpoint_large}}px;
$grid-widescreen: {{settings.display_site_width}}px;

$small: 'small';
$medium: 'medium';
$medium-down: 'medium-down';
$medium-up: 'medium-up';
$large: 'large';
$large-down: 'large-down';
$large-up: 'large-up';
$widescreen: 'widescreen';

// The `$breakpoints` list is used to build our media queries.
// You can use these in the media-query mixin.
$breakpoints: (
  $small '(max-width: #{$grid-medium - 1})',
  $medium '(min-width: #{$grid-medium}) and (max-width: #{$grid-large - 1})',
  $medium-down '(max-width: #{$grid-large - 1})',
  $medium-up '(min-width: #{$grid-medium})',
  $large '(min-width: #{$grid-large}) and (max-width: #{$grid-widescreen - 1})',
  $large-down '(max-width: #{$grid-widescreen - 1})',
  $large-up '(min-width: #{$grid-large})',
  $widescreen '(min-width: #{$grid-widescreen})'
);

/*================ Z-Index ================*/
$z-index-dropdown: 2;
$z-index-skip-to-content: 10000; // really high to be safe of app markup


