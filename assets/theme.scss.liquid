  input,
  textarea,
  select {
    font-size: 16px;
  }
}

button,
input[type="submit"],
label[for] {
  cursor: pointer;
}

optgroup {
  font-weight: $font-weight-bold;
}

// Force option color (affects IE only)
// option {
//   color: $color-primary;
//   background-color: $color-secondary;
// }

// hide outline on focus for elements which are given focus by JS
